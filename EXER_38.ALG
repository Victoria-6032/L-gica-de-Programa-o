Algoritmo "exer_38"

Var

   temp, velo, lit_usa, dist: real

Inicio

    Escreval(" Entre com tempo da viagem: ")
    leia(temp)
    Escreval(" Entre com a velocidade: ")
    Leia(velo)
    dist:= temp*velo
    lit_usa:= dist/12
    Escreva(" A quantidade de Litros usados: ", lit_usa)

Fimalgoritmo